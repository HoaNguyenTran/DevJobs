{
  "name": "DevJob",
  "version": "1.0.0",
  "description": "DevJob",
  "title": "Fjob",
  "author": "hoatep",
  "keywords": [
    "react",
    "nextjs",
    "nextjs-starter",
    "nextjs-template",
    "typescript",
    "redux",
    "redux-saga",
    "ant-design",
    "styled-components",
    "storybook",
    "jest",
    "enzyme"
