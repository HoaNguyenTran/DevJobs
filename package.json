  "repository": {
    "type": "git",
    "url": "https://github.com/zetajsc/fjob-frontend"
  },
  "license": "MIT",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:staging": "env-cmd -f .env.staging yarn build",
    "build:production": "env-cmd -f .env.production yarn build",
    "start": "next start",
    "postbuild": "cross-env next-sitemap --config next-sitemap.js",
    "docker:start:dev": "docker compose -f docker-compose.yaml up",
    "commit": "git-cz",
    "test": "jest",
    "coverage": "yarn jest --coverage",
    "lint": "yarn lint:eslint && yarn lint:css",
    "lint:eslint": "eslint --ext js,jsx,ts,tsx .",
    "lint:css": "stylelint **/*.{ts,tsx}",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "tsc:client": "tsc --noEmit -p tsconfig.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -o ./dist_storybook",
    "analyze": "cross-env ANALYZE=true next build",
    "bump-version": "standard-version --skip.commit --skip.tag",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --record --key 2d237104-b1f8-4652-b63a-cbe6a39e2602"
  },
  "lint-staged": {
    "*.{tsx,ts,js,jsx}": [
      "stylelint",
      "prettier --write",
      "eslint --fix"
    ],
    "*.{md,json,yaml,yml}": [
      "prettier --write"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
